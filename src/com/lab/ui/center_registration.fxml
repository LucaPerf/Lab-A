<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.lab.ui.CenterRegPage"
      spacing="12.0"
      alignment="CENTER">
    <TextField
            fx:id="name"
            maxWidth="200.0"
            promptText="Name"/>
    <ComboBox
            fx:id="type"
            maxWidth="200.0"/>
    <TextField
            fx:id="address"
            promptText="Indirizzo"
            maxWidth="200.0"/>
    <TextField
            fx:id="city"
            promptText="Comune"
            maxWidth="200.0"/>
    <TextField
            fx:id="cap"
            promptText="CAP"
            maxWidth="200.0"/>
    <HBox
            spacing="12.0"
            alignment="CENTER">
        <Button
                text="ANNULLA"
                fx:id="back"/>
        <Button
                text="AGGIUNGI"
                fx:id="register"/>
    </HBox>
</VBox>
